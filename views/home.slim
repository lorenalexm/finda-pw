h3 Copyright 2016

button id="gen_button" Generate

table id="passwords"

p id="copyright_slug"

coffee:
  (($) ->
    $.ajax
      url: '/api/version'
      type: 'get'
      dataType: 'json'
      error: (jqXhr, status, error) ->
        console.log error
      success: (data) ->
        markup = 'The Finda.pw application version ' + data["version"] + ' was created with love by Alex Loren'
        $('#copyright_slug').prepend markup
  ) jQuery

coffee:
  (($) ->
    $('#gen_button').click ->
      $.ajax
        url: '/api/generate'
        type: 'get'
        dataType: 'json'
        error: (jqXhr, status, error) ->
          console.log error
        success: (data) ->
          markup = '<tr>'
          markup += '<td>' + data["password"] + '</td>'
          markup += '</tr>'
          $(markup).prependTo('#passwords')
  ) jQuery